{
    "name": "Flat Black",
    "type": "dark",
    "colors": {
        // Button Control
        "button.background": "#111111",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#222222",
        "checkbox.background": "#000000",
        "checkbox.foreground": "#ffffff",
        "checkbox.border": "#222222",
        // Dropdown control
        "dropdown.background": "#000000",
