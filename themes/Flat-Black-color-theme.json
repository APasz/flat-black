        "scrollbarSlider.hoverBackground": "#222222",
        // Badge
        "badge.foreground": "#ffffff",
        "badge.background": "#222222",
        // Progress bar
        "progressBar.background": "#222222",
        // Lists and tree
        "list.activeSelectionBackground": "#222222",
